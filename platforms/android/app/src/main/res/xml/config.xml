<?xml version='1.0' encoding='utf-8'?>
<widget id="com.farewell.sept" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Farewell Ajinkya</name>
    <description>
    </description>
    <author email="bhaveshy11@gmail.com" href="https://www.cse.iitb.ac.in/~bhaveshy">
        Bhaveshkumar Yadav
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application">
        <application android:usesCleartextTraffic="true" />
    </edit-config>
    <icon density="mdpi" src="assets/mdpi.png" />
    <icon density="ldpi" src="assets/ldpi.png" />
    <icon density="hdpi" src="assets/hdpi.png" />
    <icon density="xhdpi" src="assets/xdpi.png" />
    <icon density="xxhdpi" src="assets/xxdpi.png" />
    <preference name="loglevel" value="DEBUG" />
    <preference name="AndroidPersistentFileLocation" value="Internal" />
</widget>
